{
  "name": "loteriAI-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Landing Page + multi-funnel apps em monorepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:lp": "npm run dev --workspace @loteriai/lp",
    "dev:quiz-classic": "npm run dev --workspace @loteriai/quiz-classic",
    "dev:mega": "npm run dev --workspace @loteriai/mega-quiz",
    "dev:lotozap": "npm run dev --workspace @loteriai/lotozap-quiz",
    "build:lp": "npm run build --workspace @loteriai/lp",
    "build:quiz-classic": "npm run build --workspace @loteriai/quiz-classic",
    "build:mega": "npm run build --workspace @loteriai/mega-quiz",
    "build:lotozap": "npm run build --workspace @loteriai/lotozap-quiz",
    "build:organize": "node scripts/organize-dist.js",
    "build": "npm run build:lp && npm run build:quiz-classic && npm run build:mega && npm run build:lotozap && npm run build:organize"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Bruno Falci",
  "license": "MIT"
}
